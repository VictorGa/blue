{
  "root": true,
  "parser": "babel-eslint",
  "extends": "standard",
  "plugins": [
    "html"
  ],
  "ecmaFeatures": {
    "module": true,
  },
  "settings": {
    "import/resolver": "webpack",
  },
  "rules": {
    "comma-dangle": [2,"never"],
    "no-console": 1,
    "no-debugger": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": [2,"functions"],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "block-scoped-var": 2,
    "consistent-return": 2,
    "curly": [2,"all"],
    "default-case": 2,
    "eqeqeq": [2,"smart"],
    "guard-for-in": 1,
    "no-alert": 1,
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-eval": 1,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": [2,{"boolean":false,"number":true,"string":true}],
    "no-implied-eval": 2,
    "no-invalid-this": 2,
    "no-labels": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 2,
    "no-redeclare": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-useless-call": 2,
    "no-with": 2,
    "vars-on-top": 2,
    "no-delete-var": 2,
    "no-shadow-restricted-names": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "array-bracket-spacing": [2,"never",{}],
    "brace-style": [2,"1tbs",{}],
    "camelcase": [2,{"properties":"always"}],
    "comma-spacing": [2,{"after":true}],
    "computed-property-spacing": [2,"never"],
    "eol-last": 2,
    "func-names": 2,
    "indent": [2,2],
    "key-spacing": [2,{"afterColon":true}],
    "lines-around-comment": [2,{"beforeBlockComment":true,"beforeLineComment":true,"allowBlockStart":true,"allowBlockEnd":true}],
    "new-cap": [2,{"newIsCap":true,"capIsNew":true}],
    "new-parens": 2,
    "newline-after-var": [2,"always"],
    "no-inline-comments": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": [2,"smart-tabs"],
    "no-multiple-empty-lines": [2,{"max":2}],
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-ternary": 2,
    "no-trailing-spaces": [2,{"skipBlankLines":true}],
    "no-unneeded-ternary": 2,
    "object-curly-spacing": [2,"always",{}],
    "one-var": [2,"never"],
    "operator-linebreak": [2,"before"],
    "padded-blocks": [2,"never"],
    "quote-props": [2,"consistent-as-needed"],
    "quotes": [2,"single"],
    "semi-spacing": [2,{"after":true}],
    "semi": [2,"never"],
    "space-before-blocks": [2,"always"],
    "space-before-function-paren": [2,"always"],
    "space-in-parens": [2,"never"],
    "space-infix-ops": [2,{"int32Hint":true}],
    "spaced-comment": [2,"always",{}],
    "arrow-parens": [2,"as-needed"],
    "arrow-spacing": [2,{"before":true,"after":true}],
    "no-const-assign": 2,
    "no-this-before-super": 2,
    "no-var": 2,
    "object-shorthand": [2,"always"],
    "prefer-const": 2,
    "max-len": [2,120,2,{}]
  }
}
